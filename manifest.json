{
  "manifest_version": 2,
  "name": "WhatsappWeb Dark",
  "description": "This extension will allow your eyes to rest while using whatsapp web",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}